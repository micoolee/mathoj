.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 100%;
  justify-content: space-between;
  box-sizing: border-box;
}


button::after {
  border: none;
}
button {
  background-color: #fff;
  line-height: 1.8;
  padding-left: 0px;
}

.list-item {
  display: flex;
  flex-direction: row;
  height: 88rpx;
  width: 100%;
  background-color: #fff;
  border-bottom: 1rpx solid #E5E5E5;
}

.list-name {
  margin-left: 32rpx;
  line-height: 88rpx;
  font-family: PingFang SC;
  font-size: 34rpx;
  color: #000;
  
}

.list-icon {
  margin-top: 30rpx;
  margin-left: 538rpx;
  width: 16rpx;
  height: 26rpx;
}

.list-container {
  margin-top: 98rpx;
  width: 100%;
}












